	<data noupdate="1">
		<record id="menu_camp_registration" model="website.menu">
			<field name="name">Camp Registration</field>
			<field name="url">/campregistration</field>
			<field name="parent_id" ref="website.main_menu"/>
			<field name="sequence" type="int">60</field>		
		</record>
        <record id="action_open_website" model="ir.actions.act_url">
            <field name="name">Camp Registration</field>
            <field name="target">self</field>
            <field name="url">/campregistration</field>
        </record>

        <record id="base.open_menu" model="ir.actions.todo">
            <field name="action_id" ref="action_open_website"/>
            <field name="state">open</field>
        </record>
        
        <!-- Sequences for Camp Registration  -->
        <record id="seq_camp_code" model="ir.sequence">
            <field name="name">Camp Registration</field>
            <field name="code">camp.registration</field>
            <field name="prefix">CR%(year)s%(month)s%(day)s</field>
            <field name="padding">3</field>
   			<field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
        
        
    </data>
</odoo>

